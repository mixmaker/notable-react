{
    "version": 2,
    "builds": [
        {
            "src": "api/*",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/keepActive",
            "schedule": "0 0 * * *"
        }
    ]
}